{
  "name": "server-ts",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@tsed/ajv": "5.52.2",
    "@tsed/common": "5.52.2",
    "@tsed/core": "5.65.7",
    "@tsed/di": "5.52.2",
    "@tsed/mongoose": "5.52.2",
    "@types/mongoose": "5.7.15",
    "ajv": "6.12.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.2.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "method-override": "3.0.0",
    "mongoose": "5.9.13"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "1.11.3",
    "@tsed/cli-plugin-mongoose": "1.11.3",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/hapi__joi": "^17.1.0",
    "@types/method-override": "0.0.31",
    "@types/node": "13.13.5",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "concurrently": "5.2.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "nodemon": "2.0.3",
    "prettier": "2.0.5",
    "ts-node": "8.10.1",
    "typescript": "3.8.3"
  }
}
